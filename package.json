    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.22",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "^3.2.47",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.6",
